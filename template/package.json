{
  "name": "{{ name }}",
  {{#if_eq builder 'packager'}}
    "productName": "{{ name }}",
  {{/if_eq}}
  "version": "{{ appver }}",
  "author": "{{ author }}",
  "description": "{{ description }}",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    {{#if_eq builder 'packager'}}
    "build": "node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    {{else}}
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    {{/if_eq}}
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    {{#if e2e}}
    "e2e": "npm run pack && mocha test/e2e",
    {{/if}}
    {{#if eslint}}
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src{{#testing unit e2e}} test{{/testing}}",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src{{#testing unit e2e}} test{{/testing}}",
    {{/if}}
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    {{#testing unit e2e}}
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/testing}}
    {{#if unit}}
    "unit": "karma start test/unit/karma.conf.js",
    {{/if}}
    "postinstall": "{{#if eslint}}npm run lint:fix{{/if}}"
  },
  {{#if_eq builder 'builder'}}
    "build": {
      "productName": "{{ name }}",
      "appId": "{{ appid }}",
      "directories": {
        "output": "build"
      },
      "files": [
        "dist/electron/**/*"
      ],
      "dmg": {
        "contents": [
          {
            "x": 410,
            "y": 150,
            "type": "link",
            "path": "/Applications"
          },
          {
            "x": 130,
            "y": 150,
            "type": "file"
          }
        ]
      },
      "mac": {
        "icon": "build/icons/icon.icns"
      },
      "win": {
        "icon": "build/icons/icon.ico"
      },
      "linux": {
        "icon": "build/icons"
      }
    },
  {{/if_eq}}
  "dependencies": {
    "vue": "^2.6.10"{{deps plugins}}{{depsp framework}}
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.1.6",
    "css-loader": "^3.1.0",
    "del": "^5.0.0",
    "devtron": "^1.4.0",
    "electron": "^5.0.7",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    {{#if_eq builder 'packager'}}
    "electron-packager": "^14.0.2",
    "electron-rebuild": "^1.8.1",
    {{else}}
    "electron-builder": "^21.1.1",
    {{/if_eq}}
    {{#eslint}}
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^6.0.0",
    {{#if_eq eslintConfig 'standard'}}
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    {{/if_eq}}
    {{#if_eq eslintConfig 'airbnb'}}
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.12.0",
    {{/if_eq}}
    {{/eslint}}
    "mini-css-extract-plugin": "^0.8.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    {{#if unit}}
    "inject-loader": "^4.0.1",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    {{/if}}
    {{#if e2e}}
    "require-dir": "^1.0.0",
    "spectron": "^7.0.0",
    {{/if}}
    {{#testing unit e2e}}
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.1.2",
    "mocha": "^6.2.0",
    {{/testing}}
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    {{#if_eq csspreprocessor 'sass'}}
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    {{/if_eq}}
    {{#if_eq csspreprocessor 'less'}}
    "less": "^3.9.0"
    "less-loader": "^5.0.0"
    {{/if_eq}}
    {{#if_eq csspreprocessor 'stylus'}}
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    {{/if_eq}}
    "style-loader": "^0.23.1",
    "url-loader": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
